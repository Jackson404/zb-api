
page{
	width: 100%;
	height: 100%;
	background: #fff;
}
.cu-form-group .title {
	min-width: -webkit-calc(4em + 15px);
	min-width: calc(4em + 15px);
}
input{
	text-align: right;
}
.cu-form-group+.cu-form-group{
	border-top: none;
	border-bottom: 1px solid #eee !important;
}
.cu-form-group picker .picker {
		line-height: 100rpx;
		font-size: 30rpx;
		-o-text-overflow: ellipsis;
		text-overflow: ellipsis;
		white-space: nowrap;
		overflow: hidden;
		width: 100%;
		text-align: right;
		color: #666;
}



